#define _GNU_SOURCE
#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {
  FILE *stream;
  char *line = NULL;
  size_t len = 0;
  ssize_t nread;
  stream = fopen(argv[1], "r");

  while ((nread = getline(&line, &len, stream)) != -1) {
    fwrite(line, nread, 1, stdout);
  }

  free(line);
  fclose(stream);
}
